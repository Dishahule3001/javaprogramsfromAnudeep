package javacourse;

public class greaternumber {

	public static void main(String[] args) {
		int a=34,b=43,c=21;
		if(a>b && a>c) {
					
			System.out.println("a is greater");
		}
		else if(b>a && b>c) {
			System.out.println("b is greater");
			
		}
		else {
			System.out.println("c is greater");
		}

	}

}
